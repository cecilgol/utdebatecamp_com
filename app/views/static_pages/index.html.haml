- @camps.each do |camp|
  %h2= camp.name
  %p= camp.info
  %p 
    %dt camp begins
    %dd=camp.start_date
    %dt camp ends
    %dd=camp.end_date
  
  =link_to "Sign Up",apply_to_attend_camp_path(camp.nickname),{class:'btn btn-primary'}




