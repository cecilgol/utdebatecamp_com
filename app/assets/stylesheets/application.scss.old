button {
    @extend %extend_2;
    overflow: visible;
    text-transform: none;
    -webkit-appearance: button;
    cursor: pointer;
    font-family: inherit;
    font-size: inherit;
    line-height: inherit;
    &.close {
        padding: 0;
        cursor: pointer;
        background: transparent;
        border: 0;
        -webkit-appearance: none;
    }
    &.list-group-item {
        color: $color_fuscous_gray_approx;
        width: 100%;
        text-align: left;
        .list-group-item-heading {
            color: $color_mine_shaft_approx;
