    width: 100%;
    > {
        .item {
            display: none;
            position: relative;
            //Instead of the line below you could use @include transition($transition-1, $transition-2, $transition-3, $transition-4, $transition-5, $transition-6, $transition-7, $transition-8, $transition-9, $transition-10)
            transition: 0.6s ease-in-out left;
            >img {
                display: block;
                max-width: 100%;
                height: auto;
                line-height: 1;
            }
            >a>img {
                display: block;
                max-width: 100%;
                height: auto;
                line-height: 1;
            }
        }
        .active {
            display: block;
            left: 0;
            &.left {
                left: -100%;
            }
            &.right {
                left: 100%;
            }
        }
        .next {
            display: block;
            position: absolute;
            top: 0;
            width: 100%;
            left: 100%;
            &.left {
                left: 0;
            }
        }
        .prev {
            display: block;
            position: absolute;
            top: 0;
            width: 100%;
            left: -100%;
            &.right {
                left: 0;
            }
        }
    }
}

