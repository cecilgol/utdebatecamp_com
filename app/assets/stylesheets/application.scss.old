a {
    background-color: transparent;
    color: $color_starship_approx;
    text-decoration: none;
    &:active {
        outline: 0;
    }
    &:hover {
        outline: 0;
        color: $color_bird_flower_approx;
        text-decoration: underline;
    }
    &:focus {
        color: $color_bird_flower_approx;
        text-decoration: underline;
        outline: 5px auto -webkit-focus-ring-color;
        outline-offset: -2px;
    }
    &.text-primary {
        &:hover {
            color: $color_sunflower_approx;
        }
        &:focus {
