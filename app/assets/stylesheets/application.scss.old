.sign-up-link-btn {
    float: right;
}

.bg-white {
    background-color: $color_alabaster_approx;
}

.staff {
    list-style: none;
    >li {
        background-position: center top;
        background-repeat: none;
        background-size: cover;
        border: 5px rgb(21, 21, 21) solid;
        float: left;
        height: 0;
        padding-bottom: 50%;
        position: relative;
        width: 50%;
        >.staff-member {
            background-color: rgba(21, 21, 21, 0.5);
            height: 100%;
            bottom: 0;
            position: absolute;
            transition: all .2s ease-in-out;
            -moz-transition: all .2s ease-in-out;
            -webkit-transition: all .2s ease-in-out;
            width: 100%;
            >.staff-name {
                margin: 50% auto 0;
                padding: 0 10%;
                transition: all .2s ease-in-out;
                -moz-transition: all .2s ease-in-out;
                -webkit-transition: all .2s ease-in-out;
            }
            >.biog {
                visibility: hidden;
                margin: 10% auto 0;
                padding: 0 10%;
                overflow: hidden;
                transition: all .2s ease-in-out;
                -moz-transition: all .2s ease-in-out;
                -webkit-transition: all .2s ease-in-out;
            }
        }
    }
}

@media (min-width: 992px) {
    .staff {
        >li {
            width: 33%;
            >.staff-member {
                opacity: 0;
            }
            &:hover {
                >.staff-member {
                    background-color: rgba(21, 21, 21, 0.8);
                    opacity: 1;
                    >.biog {
                        visibility: visible;
                    }
                }
            }
        }
    }
}

@media (min-width: 1200px) {
    .staff {
        >li {
            width: 25%;
            >.staff-member {
                opacity: 0;
            }
            &:hover {
                >.staff-member {
                    background-color: rgba(21, 21, 21, 0.8);
                    opacity: 1;
                    >.biog {
                        visibility: visible;
                    }
                }
            }
        }
    }
}

.text-white {
    color: white;
}

.pic {
    position: relative;
    width: 200px;
    height: 200px;
    .bg {
        position: absolute;
        top: 0px;
        left: 0px;
        bottom: 0px;
        width: 100%;
    }
    .overlay {
        position: absolute;
        top: 0px;
        left: 0px;
        bottom: 0px;
        width: 100%;
        opacity: 0;
        background: rgba(0, 0, 0, 0.5);
        color: white;
        >h3 {
            font-family: $font_20;
            padding-top: 25px;
            text-align: center;
        }
    }
    &:hover {
        .bg {
            -webkit-filter: blur(2px);
            filter: blur(2px);
        }
        .overlay {
            opacity: 1;
        }
    }
}

input#employee_avatar {
    width: 0.1px;
    height: 0.1px;
    opacity: 0;
    overflow: hidden;
    position: absolute;
    z-index: -1;
}