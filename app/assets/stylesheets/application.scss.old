        &:hover {
            color: $color_killarney_approx;
            background-color: $color_tea_green_approx;
        }
        &:focus {
            color: $color_killarney_approx;
            background-color: $color_tea_green_approx;
        }
        &.active {
            color: $white;
            background-color: $color_killarney_approx;
