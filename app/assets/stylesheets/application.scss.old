.panel-body {
    padding: 30px;
    &:before {
        content: " ";
        display: table;
    }
    &:after {
        content: " ";
        display: table;
        clear: both;
    }
}

.panel-heading {
    padding: 10px 15px;
    border-bottom: 1px solid transparent;
    //Instead of the line below you could use @include border-top-right-radius($radius)
    border-top-right-radius: -1;
    //Instead of the line below you could use @include border-top-left-radius($radius)
    border-top-left-radius: -1;
    >.dropdown .dropdown-toggle {
        color: inherit;
    }
    +.list-group .list-group-item:first-child {
        border-top-width: 0;
    }
}

.panel-title {
    margin-top: 0;
    margin-bottom: 0;
    font-size: 16px;
    color: inherit;
    >a {
        color: inherit;
    }
    >small {
        color: inherit;
        >a {
            color: inherit;
        }
    }
    >.small {
        color: inherit;
        >a {
            color: inherit;
        }
    }
}

.panel-footer {
    padding: 10px 15px;
    background-color: $color_wild_sand_approx;
    border-top: 1px solid $color_alto_approx;
    //Instead of the line below you could use @include border-bottom-right-radius($radius)
    border-bottom-right-radius: -1;
    //Instead of the line below you could use @include border-bottom-left-radius($radius)
    border-bottom-left-radius: -1;
}

.panel-group {
    margin-bottom: 22px;
    .panel {
        margin-bottom: 0;
        //Instead of the line below you could use @include border-radius($radius, $vertical-radius)
        border-radius: 0;
        +.panel {
            margin-top: 5px;
        }
    }
    .panel-heading {
        border-bottom: 0;
        +.panel-collapse> {
            .panel-body {
                border-top: 1px solid $color_alto_approx;
            }
            .list-group {
                border-top: 1px solid $color_alto_approx;
            }
        }
    }
    .panel-footer {
        border-top: 0;
        +.panel-collapse .panel-body {
            border-bottom: 1px solid $color_alto_approx;
        }
    }
}

