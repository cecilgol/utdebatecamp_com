(function() {
    $('.date').click(function() {
        showCreateEventDialog($(this));
    });

    function showCreateEventDialog(element) {
        $.get('/admin/e/modal',function(res){
            $('#m').html(res);
            $('#utnif-modal').modal('toggle');
        });
        
        $(element).css("color", "red");
    }
})(jQuery);